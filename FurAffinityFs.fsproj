<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Isaac Schemm</Authors>
    <Description>A library that lets you upload artwork to furaffinity.net</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright></Copyright>
    <PackageProjectUrl>https://github.com/IsaacSchemm/FurAffinityFs</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="FurAffinityCredentials.fs" />
    <Compile Include="FurAffinityFile.fs" />
    <Compile Include="FurAffinityCategory.fs" />
    <Compile Include="FurAffinityType.fs" />
    <Compile Include="FurAffinitySpecies.fs" />
    <Compile Include="FurAffinityGender.fs" />
    <Compile Include="FurAffinityRating.fs" />
    <Compile Include="FurAffinitySubmission.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FSharp.Data" Version="3.3.3" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="5.0.0" />
  </ItemGroup>

</Project>
